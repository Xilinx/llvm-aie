    %0:ep_as_32bit = COPY $p0
    %1:mss = COPY $s0
    $wl0 = VLDA_dmw_lda_w_ag_idx_imm %0, 0 :: (load (<8 x s32>))
    $wh0 = VLDA_dmw_lda_w_ag_idx_imm %0, 32 :: (load (<8 x s32>))
    %2:ep_as_32bit = COPY %0
    %2:ep_as_32bit = PADD_imm_pseudo %2, 64
