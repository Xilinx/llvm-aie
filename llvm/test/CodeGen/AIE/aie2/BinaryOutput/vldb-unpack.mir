    $x3 = VLDB_UNPACK_D16_D8_ag_idx $p2, $dj3, implicit $crunpacksign
    ; VLDB.UNPACK - ag_pstm_nrm
    $x4, $p6 = VLDB_UNPACK_S8_S4_ag_pstm_nrm $p6, $m0
    $x5, $p0 = VLDB_UNPACK_S16_S8_ag_pstm_nrm $p0, $m1
    $x6, $p3 = VLDB_UNPACK_D8_D4_pstm_nrm $p3, $m2, implicit $crunpacksign
    $x7, $p1 = VLDB_UNPACK_D16_D8_ag_pstm_nrm $p1, $m3, implicit $crunpacksign
    ; VLDB.UNPACK - ag_pstm_2d
    $x4, $p0, $dc0 = VLDB_2D_UNPACK_S8_S4 $p0, $d0
    $x5, $p1, $dc1 = VLDB_2D_UNPACK_S16_S8 $p1, $d1
    $x6, $p2, $dc2 = VLDB_2D_UNPACK_D8_D4 $p2, $d2, implicit $crunpacksign
    $x7, $p3, $dc3 = VLDB_2D_UNPACK_D16_D8 $p3, $d3, implicit $crunpacksign
    ; VLDB.UNPACK - ag_pstm_3d
    $x4, $p4, $dc0, $dc4 = VLDB_3D_UNPACK_S8_S4 $p4, $d0_3d
    $x5, $p5, $dc1, $dc5 = VLDB_3D_UNPACK_S16_S8 $p5, $d1_3d
    $x6, $p6, $dc2, $dc6 = VLDB_3D_UNPACK_D8_D4 $p6, $d2_3d, implicit $crunpacksign
    $x7, $p7, $dc3, $dc7 = VLDB_3D_UNPACK_D16_D8 $p7, $d3_3d, implicit $crunpacksign

