    ; VNEGADD.f / VNEGSUB.f – Floating-point accumulator negated
    $bml5 = VNEGADD_F $bml5, $bml2, $r3, implicit-def $srfpflags, implicit $crfpmask
    $bml5 = VNEGSUB_F $bml5, $bml2, $r3, implicit-def $srfpflags, implicit $crfpmask
