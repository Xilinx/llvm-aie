    VST_dmw_sts_w_ag_idx $wh6, $p5, $dj4

    ; VST - Vector Stores - ag_pstm_nrm
    $p2 = VST_dmw_sts_w_ag_pstm_nrm $wl3, $p2, $m4
    $p2 = VST_dmw_sts_w_ag_pstm_nrm $wl7, $p2, $m3
    $p5 = VST_dmw_sts_w_ag_pstm_nrm $wh4, $p5, $m2
    $p5 = VST_dmw_sts_w_ag_pstm_nrm $wh6, $p5, $m0

    ; VST - Vector Stores - ag_pstm_nrm_imm
    $p2 = VST_dmw_sts_w_ag_pstm_nrm_imm $wl3, $p2, 0
    $p2 = VST_dmw_sts_w_ag_pstm_nrm_imm $wl7, $p2, -1056
    $p5 = VST_dmw_sts_w_ag_pstm_nrm_imm $wh4, $p5, 2016
    $p5 = VST_dmw_sts_w_ag_pstm_nrm_imm $wh6, $p5, -2048

    ; VST - Vector Stores - ag_pstm_2d
    $p2, $dc2 = VST_2D_dmw_sts_w $wl3, $p2, $d2
    $p2, $dc1 = VST_2D_dmw_sts_w $wl7, $p2, $d1
    $p5, $dc0 = VST_2D_dmw_sts_w $wh4, $p5, $d0
    $p5, $dc4 = VST_2D_dmw_sts_w $wh6, $p5, $d4

    ; VST - Vector Stores - ag_pstm_3d
    $p0, $dc0, $dc4 = VST_3D_dmw_sts_w $wl3, $p0, $d0_3d
    $p2, $dc1, $dc5 = VST_3D_dmw_sts_w $wl7, $p2, $d1_3d
    $p4, $dc2, $dc6 = VST_3D_dmw_sts_w $wh4, $p4, $d2_3d
    $p5, $dc3, $dc7 = VST_3D_dmw_sts_w $wh6, $p5, $d3_3d
...

