body:             |
  bb.0 (align 16):
    ; VST - Vector Stores - ag_idx_imm
    VST_dmw_sts_w_ag_idx_imm $wl3, $p2, 0
    VST_dmw_sts_w_ag_idx_imm $wl7, $p2, 32
    VST_dmw_sts_w_ag_idx_imm $wh4, $p5, 992
    VST_dmw_sts_w_ag_idx_imm $wh6, $p5, -1024

    ; VST - Vector Stores - ag_spill
    VST_dmw_sts_w_ag_spill $wl3, -32, implicit $sp
    VST_dmw_sts_w_ag_spill $wl7, -131072, implicit $sp
    VST_dmw_sts_w_ag_spill $wh4, -64, implicit $sp
    VST_dmw_sts_w_ag_spill $wh6, -131072, implicit $sp

    ; VST - Vector Stores - ag_idx
    VST_dmw_sts_w_ag_idx $wl3, $p2, $dj2
    VST_dmw_sts_w_ag_idx $wl7, $p2, $dj1
